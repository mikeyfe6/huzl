{
    "cli": {
        "version": ">= 16.28.0",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "GOOGLE_SERVICES_JSON": "@env GOOGLE_SERVICES_JSON",
                "GOOGLE_SERVICE_INFO_PLIST": "@env GOOGLE_SERVICE_INFO_PLIST"
            }
        },
        "preview": {
            "distribution": "internal"
        },
        "production": {
            "autoIncrement": true
        }
    },
    "submit": {
        "production": {}
    }
}
