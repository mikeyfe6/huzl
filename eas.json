{
    "cli": {
        "version": ">= 16.28.0",
        "appVersionSource": "remote"
    },

    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "ios": {
                "googleServicesFile": "./ios/huzl/GoogleService-Info.plist"
            },
            "android": {
                "googleServicesFile": "./android/app/google-services.json"
            }
        },
        "preview": {
            "distribution": "internal",
            "ios": {
                "googleServicesFile": "./ios/huzl/GoogleService-Info.plist"
            },
            "android": {
                "googleServicesFile": "./android/app/google-services.json"
            }
        },
        "production": {
            "autoIncrement": true,
            "ios": {
                "googleServicesFile": "./ios/huzl/GoogleService-Info.plist"
            },
            "android": {
                "googleServicesFile": "./android/app/google-services.json"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
